  "slider-layout#outros-audiobooks": {
    "props": {
      "itemsPerPage": {
        "desktop": 5,
        "tablet": 3,
        "phone": 1
      },
      "infinite": true,
      "showNavigationArrows": "desktopOnly",
      "blockClass": "outros-audiobooks"
    }

  },
  "flex-layout.row#outros-audiobooks-titulo":{
    "children":["rich-text#outros-audiobooks"],
    "props":{

      "blockClass":"outros-audiobooks-titulo"
    }
  },
  "rich-text#outros-audiobooks": {
    "props": {
      "textAlignment":"Center",
      "text": "## Outros Audiobooks\nEscolha abaixo e ouça",
      "blockClass": "outros-audiobooks-titulo"
    }
  },

  "info-card#home": {
    "props": {
      "id": "info-card-home",
      "isFullModeStyle": false,
      "imageUrl": "assets/img/banners-inferior-desktop.png",
      "mobileImageUrl":"assets/img/banners-mob.png",
      "blockClass": "info-card-home"
    }
  },
  "flex-layout.row#booklists": {
    "children": ["list-context.image-list#booklists"],
